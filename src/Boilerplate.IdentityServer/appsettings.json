{
    "Serilog": {
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
            }
        },
        "WriteTo": [
            {
                "Name": "Async",
                "Args": {
                    "configure": [
                        {
                            "Name": "Console",
                            "Args": {
                                "theme": "Serilog.Sinks.SystemConsole.Themes.ConsoleTheme::None, Serilog.Sinks.Console",
                                "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzzz}] [{Level:u3}] {Message:lj}{NewLine}{Exception}"
                            }
                        }
                    ]
                }
            }
        ]
    }
}
